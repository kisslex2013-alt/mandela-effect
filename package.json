{
  "name": "mandela-effect",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "dev:clean": "npm run clean && next dev --webpack",
    "build": "prisma generate && next build --webpack",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "eslint",
    "clean": "node -e \"const fs = require('fs'); const path = require('path'); function rmDir(dir) { if (fs.existsSync(dir)) { fs.readdirSync(dir).forEach(f => { const curPath = path.join(dir, f); if (fs.lstatSync(curPath).isDirectory()) rmDir(curPath); else fs.unlinkSync(curPath); }); fs.rmdirSync(dir); } } rmDir('.next'); console.log('✅ Кеш очищен');\"",
    "analyze": "cross-env ANALYZE=true next build --webpack",
    "db:seed": "npx prisma db seed",
    "db:reset-demo": "cross-env SEED_MODE=demo npx prisma db push --force-reset && cross-env SEED_MODE=demo npx prisma db seed",
    "db:reset-prod": "cross-env SEED_MODE=production npx prisma db push --force-reset && cross-env SEED_MODE=production npx prisma db seed"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^5.22.0",
    "@supabase/supabase-js": "^2.86.2",
    "@types/canvas-confetti": "^1.9.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/blob": "^2.0.0",
    "@vercel/speed-insights": "^1.3.1",
    "canvas-confetti": "^1.9.4",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "emoji-picker-react": "^4.16.1",
    "exa-js": "^2.0.11",
    "framer-motion": "^12.23.24",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.555.0",
    "next": "^16.0.7",
    "openai": "^6.9.1",
    "prisma": "^5.22.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-hot-toast": "^2.6.0",
    "react-qr-code": "^2.0.18",
    "recharts": "^3.5.1",
    "sharp": "^0.34.5",
    "uuid": "^11.1.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.0.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "^16.0.7",
    "postcss": "^8.4.49",
    "tailwindcss": "3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
